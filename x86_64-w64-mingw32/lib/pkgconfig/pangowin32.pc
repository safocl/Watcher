prefix=/usr/x86_64-w64-mingw32
libdir=${prefix}/lib
includedir=${prefix}/include

Name: Pango Win32
Description: Win32 GDI font support for Pango
Version: 1.44.7
Requires: pango
Requires.private: glib-2.0 >=  2.59.2, gobject-2.0 >=  2.59.2, fribidi >=  0.19.7, libthai >=  0.1.9, harfbuzz >=  2.0.0, fontconfig >=  2.11.91, freetype2, cairo >=  1.12.10
Libs: -L${libdir} -lpangowin32-1.0
Libs.private: -lm -lgdi32
Cflags:-I${includedir}/pango-1.0
